{
  "name": "playwrightuitests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:allure": "rimraf ./allure-results ./allure-report",
    "auth:setup": "npx playwright test --project=setup",
    "tests:only": "npx playwright test",
    "tests:full": "npm run clean:allure && npm run auth:setup && npm run tests:only",
    "report": "allure serve ./allure-results",
    "report:ci": "allure generate ./allure-results --clean -o ./allure-report",
    "test": "npm run tests:full || true && npm run report",
    "test:ci": "npm run tests:full && npm run report:ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "allure-playwright": "^3.3.3",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "allure-js-commons": "^3.3.3",
    "arg": "^4.1.3",
    "charenc": "^0.0.2",
    "create-require": "^1.1.1",
    "crypt": "^0.0.2",
    "diff": "^4.0.2",
    "dotenv": "^17.2.1",
    "is-buffer": "^1.1.6",
    "make-error": "^1.3.6",
    "md5": "^2.3.0",
    "playwright-core": "^1.54.2",
    "undici-types": "^7.10.0",
    "v8-compile-cache-lib": "^3.0.1",
    "yn": "^3.1.1"
  },
  "description": ""
}
