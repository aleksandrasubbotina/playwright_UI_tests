{
  "name": "playwrightuitests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:artifacts": "rimraf ./allure-results ./allure-report ./test-results",
    "auth:setup": "npx playwright test --project=setup",
    "test:run": "npx playwright test",
    "test:full": "npm run clean:artifacts && npm run auth:setup && npm run test:run",
    "report:generate": "rimraf ./allure-report && npx allure generate ./allure-results -o ./allure-report",
    "report:serve": "npx allure serve ./allure-results",
    "report:open": "npx allure open ./allure-report",
    "test:local": "npm run test:full; npm run report:serve",
    "test:ci": "npm run test:full && npm run report:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "allure-playwright": "^3.3.3",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "allure-js-commons": "^3.3.3",
    "arg": "^4.1.3",
    "charenc": "^0.0.2",
    "create-require": "^1.1.1",
    "crypt": "^0.0.2",
    "diff": "^4.0.2",
    "dotenv": "^17.2.1",
    "is-buffer": "^1.1.6",
    "make-error": "^1.3.6",
    "md5": "^2.3.0",
    "playwright-core": "^1.54.2",
    "undici-types": "^7.10.0",
    "v8-compile-cache-lib": "^3.0.1",
    "yn": "^3.1.1"
  },
  "description": ""
}
